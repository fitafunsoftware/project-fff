config_version=5

[application]

config/name="project-fff"
run/main_scene="res://scenes/main_game.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://proprietary/assets/art/logos/project-fff-android-icon.png"

[audio]

driver/driver=""

[autoload]

ExitShortcut="*res://autoloads/exit_shortcut.gd"
ResourceQueue="*res://autoloads/resource_queue.gd"
GlobalParams="*res://autoloads/global_params.gd"
FlipControllerMap="*res://autoloads/flip_controller_map.gd"
